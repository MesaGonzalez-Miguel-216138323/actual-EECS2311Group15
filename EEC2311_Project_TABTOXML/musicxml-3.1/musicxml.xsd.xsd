<?xml version="1.0" encoding="UTF-8"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" elementFormDefault="qualified" xmlns:xlink="http://www.w3.org/1999/xlink">
    <xs:import namespace="http://www.w3.org/1999/xlink" schemaLocation="xlink.xsd"/>
    <xs:import namespace="http://www.w3.org/XML/1998/namespace" schemaLocation="xml.xsd"/>
    <xs:element name="score-partwise">
        <xs:complexType>
            <xs:sequence>
                <xs:element ref="work"/>
                <xs:element ref="movement-number"/>
                <xs:element ref="movement-title"/>
                <xs:element ref="identification"/>
                <xs:element ref="defaults"/>
                <xs:element ref="credit"/>
                <xs:element ref="part-list"/>
                <xs:element ref="part"/>
            </xs:sequence>
            <xs:attribute name="version" use="required" type="xs:decimal"/>
        </xs:complexType>
    </xs:element>
    <xs:element name="work">
        <xs:complexType>
            <xs:sequence>
                <xs:element ref="work-number"/>
                <xs:element ref="work-title"/>
                <xs:element ref="opus"/>
            </xs:sequence>
        </xs:complexType>
    </xs:element>
    <xs:element name="work-number" type="xs:NCName"/>
    <xs:element name="work-title" type="xs:NCName"/>
    <xs:element name="opus">
        <xs:complexType>
            <xs:attribute ref="xlink:actuate" use="required"/>
            <xs:attribute ref="xlink:href" use="required"/>
            <xs:attribute ref="xlink:role" use="required"/>
            <xs:attribute ref="xlink:show" use="required"/>
            <xs:attribute ref="xlink:title" use="required"/>
            <xs:attribute ref="xlink:type" use="required"/>
        </xs:complexType>
    </xs:element>
    <xs:element name="movement-number" type="xs:NCName"/>
    <xs:element name="movement-title" type="xs:NCName"/>
    <xs:element name="defaults">
        <xs:complexType>
            <xs:sequence>
                <xs:element ref="scaling"/>
                <xs:element ref="page-layout"/>
                <xs:element ref="system-layout"/>
                <xs:element ref="staff-layout"/>
                <xs:element ref="appearance"/>
                <xs:element ref="music-font"/>
                <xs:element ref="word-font"/>
                <xs:element ref="lyric-font"/>
                <xs:element ref="lyric-language"/>
            </xs:sequence>
        </xs:complexType>
    </xs:element>
    <xs:element name="scaling">
        <xs:complexType>
            <xs:sequence>
                <xs:element ref="millimeters"/>
                <xs:element ref="tenths"/>
            </xs:sequence>
        </xs:complexType>
    </xs:element>
    <xs:element name="millimeters" type="xs:decimal"/>
    <xs:element name="tenths" type="xs:decimal"/>
    <xs:element name="appearance">
        <xs:complexType>
            <xs:sequence>
                <xs:element ref="line-width"/>
                <xs:element ref="note-size"/>
                <xs:element ref="distance"/>
                <xs:element ref="glyph"/>
                <xs:element ref="other-appearance"/>
            </xs:sequence>
        </xs:complexType>
    </xs:element>
    <xs:element name="line-width">
        <xs:complexType>
            <xs:simpleContent>
                <xs:extension base="xs:decimal">
                    <xs:attribute name="type" use="required" type="xs:NCName"/>
                </xs:extension>
            </xs:simpleContent>
        </xs:complexType>
    </xs:element>
    <xs:element name="note-size">
        <xs:complexType>
            <xs:simpleContent>
                <xs:extension base="xs:decimal">
                    <xs:attribute name="type" use="required" type="xs:NCName"/>
                </xs:extension>
            </xs:simpleContent>
        </xs:complexType>
    </xs:element>
    <xs:element name="distance">
        <xs:complexType>
            <xs:simpleContent>
                <xs:extension base="xs:decimal">
                    <xs:attribute name="type" use="required" type="xs:NCName"/>
                </xs:extension>
            </xs:simpleContent>
        </xs:complexType>
    </xs:element>
    <xs:element name="glyph">
        <xs:complexType>
            <xs:simpleContent>
                <xs:extension base="xs:NCName">
                    <xs:attribute name="type" use="required" type="xs:NCName"/>
                </xs:extension>
            </xs:simpleContent>
        </xs:complexType>
    </xs:element>
    <xs:element name="other-appearance">
        <xs:complexType>
            <xs:simpleContent>
                <xs:extension base="xs:NCName">
                    <xs:attribute name="type" use="required" type="xs:NCName"/>
                </xs:extension>
            </xs:simpleContent>
        </xs:complexType>
    </xs:element>
    <xs:element name="music-font">
        <xs:complexType>
            <xs:attribute name="font-family" use="required" type="xs:NCName"/>
            <xs:attribute name="font-size" use="required" type="xs:decimal"/>
            <xs:attribute name="font-style" use="required" type="xs:NCName"/>
            <xs:attribute name="font-weight" use="required" type="xs:NCName"/>
        </xs:complexType>
    </xs:element>
    <xs:element name="word-font">
        <xs:complexType>
            <xs:attribute name="font-family" use="required" type="xs:NCName"/>
            <xs:attribute name="font-size" use="required" type="xs:NCName"/>
            <xs:attribute name="font-style" use="required" type="xs:NCName"/>
            <xs:attribute name="font-weight" use="required" type="xs:NCName"/>
        </xs:complexType>
    </xs:element>
    <xs:element name="lyric-font">
        <xs:complexType>
            <xs:attribute name="font-family" use="required" type="xs:NCName"/>
            <xs:attribute name="font-size" use="required" type="xs:NCName"/>
            <xs:attribute name="font-style" use="required" type="xs:NCName"/>
            <xs:attribute name="font-weight" use="required" type="xs:NCName"/>
            <xs:attribute name="name" use="required" type="xs:NCName"/>
            <xs:attribute name="number" use="required" type="xs:NCName"/>
        </xs:complexType>
    </xs:element>
    <xs:element name="lyric-language">
        <xs:complexType>
            <xs:attribute name="name" use="required" type="xs:NCName"/>
            <xs:attribute name="number" use="required" type="xs:NCName"/>
            <xs:attributeGroup ref="xml:lang"/>
        </xs:complexType>
    </xs:element>
    <xs:element name="credit">
        <xs:complexType>
            <xs:sequence>
                <xs:element ref="credit-type"/>
                <xs:choice maxOccurs="unbounded">
                    <xs:element ref="bookmark"/>
                    <xs:element ref="link"/>
                    <xs:element ref="credit-image"/>
                    <xs:element ref="credit-symbol"/>
                    <xs:element ref="credit-words"/>
                </xs:choice>
            </xs:sequence>
            <xs:attribute name="id" use="required" type="xs:NCName"/>
            <xs:attribute name="page" use="required" type="xs:integer"/>
        </xs:complexType>
    </xs:element>
    <xs:element name="credit-type" type="xs:NCName"/>
    <xs:element name="credit-image">
        <xs:complexType>
            <xs:attribute name="default-x" use="required" type="xs:decimal"/>
            <xs:attribute name="default-y" use="required" type="xs:decimal"/>
            <xs:attribute name="halign" use="required" type="xs:NCName"/>
            <xs:attribute name="height" use="required" type="xs:decimal"/>
            <xs:attribute name="id" use="required" type="xs:NCName"/>
            <xs:attribute name="relative-x" use="required" type="xs:decimal"/>
            <xs:attribute name="relative-y" use="required" type="xs:decimal"/>
            <xs:attribute name="source" use="required" type="xs:anyURI"/>
            <xs:attribute name="type" use="required" type="xs:NCName"/>
            <xs:attribute name="valign" use="required" type="xs:NCName"/>
            <xs:attribute name="width" use="required" type="xs:decimal"/>
        </xs:complexType>
    </xs:element>
    <xs:element name="credit-symbol">
        <xs:complexType>
            <xs:simpleContent>
                <xs:extension base="xs:NCName">
                    <xs:attribute name="color" use="required" type="xs:NCName"/>
                    <xs:attribute name="default-x" use="required" type="xs:decimal"/>
                    <xs:attribute name="default-y" use="required" type="xs:decimal"/>
                    <xs:attribute name="dir" use="required" type="xs:NCName"/>
                    <xs:attribute name="enclosure" use="required" type="xs:NCName"/>
                    <xs:attribute name="font-family" use="required" type="xs:NCName"/>
                    <xs:attribute name="font-size" use="required" type="xs:decimal"/>
                    <xs:attribute name="font-style" use="required" type="xs:NCName"/>
                    <xs:attribute name="font-weight" use="required" type="xs:NCName"/>
                    <xs:attribute name="halign" use="required" type="xs:NCName"/>
                    <xs:attribute name="id" use="required" type="xs:NCName"/>
                    <xs:attribute name="justify" use="required" type="xs:NCName"/>
                    <xs:attribute name="letter-spacing" use="required" type="xs:NMTOKEN"/>
                    <xs:attribute name="line-height" use="required" type="xs:decimal"/>
                    <xs:attribute name="line-through" use="required" type="xs:integer"/>
                    <xs:attribute name="overline" use="required" type="xs:integer"/>
                    <xs:attribute name="relative-x" use="required" type="xs:decimal"/>
                    <xs:attribute name="relative-y" use="required" type="xs:decimal"/>
                    <xs:attribute name="rotation" use="required" type="xs:integer"/>
                    <xs:attribute name="underline" use="required" type="xs:integer"/>
                    <xs:attribute name="valign" use="required" type="xs:NCName"/>
                </xs:extension>
            </xs:simpleContent>
        </xs:complexType>
    </xs:element>
    <xs:element name="credit-words">
        <xs:complexType>
            <xs:simpleContent>
                <xs:extension base="xs:NCName">
                    <xs:attribute name="color" use="required" type="xs:NCName"/>
                    <xs:attribute name="default-x" use="required" type="xs:decimal"/>
                    <xs:attribute name="default-y" use="required" type="xs:decimal"/>
                    <xs:attribute name="dir" use="required" type="xs:NCName"/>
                    <xs:attribute name="enclosure" use="required" type="xs:NCName"/>
                    <xs:attribute name="font-family" use="required" type="xs:NCName"/>
                    <xs:attribute name="font-size" use="required" type="xs:decimal"/>
                    <xs:attribute name="font-style" use="required" type="xs:NCName"/>
                    <xs:attribute name="font-weight" use="required" type="xs:NCName"/>
                    <xs:attribute name="halign" use="required" type="xs:NCName"/>
                    <xs:attribute name="id" use="required" type="xs:NCName"/>
                    <xs:attribute name="justify" use="required" type="xs:NCName"/>
                    <xs:attribute name="letter-spacing" use="required" type="xs:NMTOKEN"/>
                    <xs:attribute name="line-height" use="required" type="xs:decimal"/>
                    <xs:attribute name="line-through" use="required" type="xs:integer"/>
                    <xs:attribute name="overline" use="required" type="xs:integer"/>
                    <xs:attribute name="relative-x" use="required" type="xs:decimal"/>
                    <xs:attribute name="relative-y" use="required" type="xs:decimal"/>
                    <xs:attribute name="rotation" use="required" type="xs:integer"/>
                    <xs:attribute name="underline" use="required" type="xs:integer"/>
                    <xs:attribute name="valign" use="required" type="xs:NCName"/>
                    <xs:attributeGroup ref="xml:lang"/>
                    <xs:attribute ref="xml:space" use="required"/>
                </xs:extension>
            </xs:simpleContent>
        </xs:complexType>
    </xs:element>
    <xs:element name="part-list">
        <xs:complexType>
            <xs:choice maxOccurs="unbounded">
                <xs:element ref="part-group"/>
                <xs:element ref="score-part"/>
            </xs:choice>
        </xs:complexType>
    </xs:element>
    <xs:element name="part-group">
        <xs:complexType>
            <xs:sequence>
                <xs:element ref="group-name"/>
                <xs:element ref="group-name-display"/>
                <xs:element ref="group-abbreviation"/>
                <xs:element ref="group-abbreviation-display"/>
                <xs:element ref="group-symbol"/>
                <xs:element ref="group-barline"/>
                <xs:element ref="group-time"/>
                <xs:element ref="footnote"/>
                <xs:element ref="level"/>
            </xs:sequence>
            <xs:attribute name="number" use="required" type="xs:integer"/>
            <xs:attribute name="type" use="required" type="xs:NCName"/>
        </xs:complexType>
    </xs:element>
    <xs:element name="group-name">
        <xs:complexType>
            <xs:simpleContent>
                <xs:extension base="xs:NCName">
                    <xs:attribute name="color" use="required" type="xs:NCName"/>
                    <xs:attribute name="default-x" use="required" type="xs:decimal"/>
                    <xs:attribute name="default-y" use="required" type="xs:decimal"/>
                    <xs:attribute name="font-family" use="required" type="xs:NCName"/>
                    <xs:attribute name="font-size" use="required" type="xs:NMTOKEN"/>
                    <xs:attribute name="font-style" use="required" type="xs:NCName"/>
                    <xs:attribute name="font-weight" use="required" type="xs:NCName"/>
                    <xs:attribute name="justify" use="required" type="xs:NCName"/>
                    <xs:attribute name="relative-x" use="required" type="xs:decimal"/>
                    <xs:attribute name="relative-y" use="required" type="xs:decimal"/>
                </xs:extension>
            </xs:simpleContent>
        </xs:complexType>
    </xs:element>
    <xs:element name="group-name-display">
        <xs:complexType>
            <xs:sequence>
                <xs:element ref="display-text"/>
                <xs:element ref="accidental-text"/>
            </xs:sequence>
            <xs:attribute name="print-object" use="required" type="xs:NCName"/>
        </xs:complexType>
    </xs:element>
    <xs:element name="group-abbreviation">
        <xs:complexType>
            <xs:simpleContent>
                <xs:extension base="xs:NCName">
                    <xs:attribute name="color" use="required" type="xs:NCName"/>
                    <xs:attribute name="default-x" use="required" type="xs:decimal"/>
                    <xs:attribute name="default-y" use="required" type="xs:decimal"/>
                    <xs:attribute name="font-family" use="required" type="xs:NCName"/>
                    <xs:attribute name="font-size" use="required" type="xs:NMTOKEN"/>
                    <xs:attribute name="font-style" use="required" type="xs:NCName"/>
                    <xs:attribute name="font-weight" use="required" type="xs:NCName"/>
                    <xs:attribute name="justify" use="required" type="xs:NCName"/>
                    <xs:attribute name="relative-x" use="required" type="xs:decimal"/>
                    <xs:attribute name="relative-y" use="required" type="xs:decimal"/>
                </xs:extension>
            </xs:simpleContent>
        </xs:complexType>
    </xs:element>
    <xs:element name="group-abbreviation-display">
        <xs:complexType>
            <xs:sequence>
                <xs:element ref="display-text"/>
                <xs:element ref="accidental-text"/>
            </xs:sequence>
            <xs:attribute name="print-object" use="required" type="xs:NCName"/>
        </xs:complexType>
    </xs:element>
    <xs:element name="group-symbol">
        <xs:complexType>
            <xs:simpleContent>
                <xs:extension base="xs:NCName">
                    <xs:attribute name="color" use="required" type="xs:NCName"/>
                    <xs:attribute name="default-x" use="required" type="xs:decimal"/>
                    <xs:attribute name="default-y" use="required" type="xs:decimal"/>
                    <xs:attribute name="relative-x" use="required" type="xs:decimal"/>
                    <xs:attribute name="relative-y" use="required" type="xs:decimal"/>
                </xs:extension>
            </xs:simpleContent>
        </xs:complexType>
    </xs:element>
    <xs:element name="group-barline">
        <xs:complexType>
            <xs:simpleContent>
                <xs:extension base="xs:NCName">
                    <xs:attribute name="color" use="required" type="xs:NCName"/>
                </xs:extension>
            </xs:simpleContent>
        </xs:complexType>
    </xs:element>
    <xs:element name="group-time">
        <xs:complexType/>
    </xs:element>
    <xs:element name="score-part">
        <xs:complexType>
            <xs:sequence>
                <xs:element ref="identification"/>
                <xs:element ref="part-name"/>
                <xs:element ref="part-name-display"/>
                <xs:element ref="part-abbreviation"/>
                <xs:element ref="part-abbreviation-display"/>
                <xs:element ref="group"/>
                <xs:element ref="score-instrument"/>
                <xs:element ref="midi-device"/>
                <xs:element ref="midi-instrument"/>
            </xs:sequence>
            <xs:attribute name="id" use="required" type="xs:NCName"/>
        </xs:complexType>
    </xs:element>
    <xs:element name="part-name">
        <xs:complexType>
            <xs:simpleContent>
                <xs:extension base="xs:NCName">
                    <xs:attribute name="color" use="required" type="xs:NCName"/>
                    <xs:attribute name="default-x" use="required" type="xs:decimal"/>
                    <xs:attribute name="default-y" use="required" type="xs:decimal"/>
                    <xs:attribute name="font-family" use="required" type="xs:NCName"/>
                    <xs:attribute name="font-size" use="required" type="xs:NCName"/>
                    <xs:attribute name="font-style" use="required" type="xs:NCName"/>
                    <xs:attribute name="font-weight" use="required" type="xs:NCName"/>
                    <xs:attribute name="justify" use="required" type="xs:NCName"/>
                    <xs:attribute name="print-object" use="required" type="xs:NCName"/>
                    <xs:attribute name="relative-x" use="required" type="xs:decimal"/>
                    <xs:attribute name="relative-y" use="required" type="xs:decimal"/>
                </xs:extension>
            </xs:simpleContent>
        </xs:complexType>
    </xs:element>
    <xs:element name="part-abbreviation">
        <xs:complexType>
            <xs:simpleContent>
                <xs:extension base="xs:NCName">
                    <xs:attribute name="color" use="required" type="xs:NCName"/>
                    <xs:attribute name="default-x" use="required" type="xs:decimal"/>
                    <xs:attribute name="default-y" use="required" type="xs:decimal"/>
                    <xs:attribute name="font-family" use="required" type="xs:NCName"/>
                    <xs:attribute name="font-size" use="required" type="xs:decimal"/>
                    <xs:attribute name="font-style" use="required" type="xs:NCName"/>
                    <xs:attribute name="font-weight" use="required" type="xs:NCName"/>
                    <xs:attribute name="justify" use="required" type="xs:NCName"/>
                    <xs:attribute name="print-object" use="required" type="xs:NCName"/>
                    <xs:attribute name="relative-x" use="required" type="xs:decimal"/>
                    <xs:attribute name="relative-y" use="required" type="xs:decimal"/>
                </xs:extension>
            </xs:simpleContent>
        </xs:complexType>
    </xs:element>
    <xs:element name="group" type="xs:NCName"/>
    <xs:element name="score-instrument">
        <xs:complexType>
            <xs:sequence>
                <xs:element ref="instrument-name"/>
                <xs:element ref="instrument-abbreviation"/>
                <xs:element ref="instrument-sound"/>
                <xs:element ref="solo"/>
                <xs:element ref="ensemble"/>
                <xs:element ref="virtual-instrument"/>
            </xs:sequence>
            <xs:attribute name="id" use="required" type="xs:NCName"/>
        </xs:complexType>
    </xs:element>
    <xs:element name="instrument-name" type="xs:NCName"/>
    <xs:element name="instrument-abbreviation" type="xs:NCName"/>
    <xs:element name="instrument-sound" type="xs:NCName"/>
    <xs:element name="solo">
        <xs:complexType/>
    </xs:element>
    <xs:element name="ensemble" type="xs:string"/>
    <xs:element name="virtual-instrument">
        <xs:complexType>
            <xs:sequence>
                <xs:element ref="virtual-library"/>
                <xs:element ref="virtual-name"/>
            </xs:sequence>
        </xs:complexType>
    </xs:element>
    <xs:element name="virtual-library" type="xs:NCName"/>
    <xs:element name="virtual-name" type="xs:NCName"/>
    <xs:element name="part">
        <xs:complexType>
            <xs:sequence>
                <xs:element ref="measure"/>
            </xs:sequence>
            <xs:attribute name="id" use="required" type="xs:NCName"/>
        </xs:complexType>
    </xs:element>
    <xs:element name="measure">
        <xs:complexType>
            <xs:sequence>
                <xs:element ref="note"/>
                <xs:element ref="backup"/>
                <xs:element ref="forward"/>
                <xs:element ref="direction"/>
                <xs:element ref="attributes"/>
                <xs:element ref="harmony"/>
                <xs:element ref="figured-bass"/>
                <xs:element ref="print"/>
                <xs:element ref="sound"/>
                <xs:element ref="barline"/>
                <xs:element ref="grouping"/>
                <xs:element ref="link"/>
                <xs:element ref="bookmark"/>
            </xs:sequence>
            <xs:attribute name="id" use="required" type="xs:NCName"/>
            <xs:attribute name="implicit" use="required" type="xs:NCName"/>
            <xs:attribute name="non-controlling" use="required" type="xs:NCName"/>
            <xs:attribute name="number" use="required" type="xs:NCName"/>
            <xs:attribute name="text" use="required" type="xs:NCName"/>
            <xs:attribute name="width" use="required" type="xs:decimal"/>
        </xs:complexType>
    </xs:element>
    <xs:element name="note">
        <xs:complexType>
            <xs:sequence>
                <xs:element ref="grace"/>
                <xs:choice maxOccurs="unbounded">
                    <xs:element ref="duration"/>
                    <xs:element ref="chord"/>
                    <xs:element ref="cue"/>
                    <xs:element ref="pitch"/>
                    <xs:element ref="rest"/>
                    <xs:element ref="tie"/>
                    <xs:element ref="unpitched"/>
                </xs:choice>
                <xs:element ref="instrument"/>
                <xs:element ref="footnote"/>
                <xs:element ref="level"/>
                <xs:element ref="voice"/>
                <xs:element ref="type"/>
                <xs:element ref="dot"/>
                <xs:element ref="accidental"/>
                <xs:element ref="time-modification"/>
                <xs:element ref="stem"/>
                <xs:element ref="notehead"/>
                <xs:element ref="notehead-text"/>
                <xs:element ref="staff"/>
                <xs:element ref="beam"/>
                <xs:element ref="notations"/>
                <xs:element ref="lyric"/>
                <xs:element ref="play"/>
            </xs:sequence>
            <xs:attribute name="attack" use="required" type="xs:decimal"/>
            <xs:attribute name="color" use="required" type="xs:NCName"/>
            <xs:attribute name="default-x" use="required" type="xs:decimal"/>
            <xs:attribute name="default-y" use="required" type="xs:decimal"/>
            <xs:attribute name="dynamics" use="required" type="xs:decimal"/>
            <xs:attribute name="end-dynamics" use="required" type="xs:decimal"/>
            <xs:attribute name="font-family" use="required" type="xs:NCName"/>
            <xs:attribute name="font-size" use="required" type="xs:NCName"/>
            <xs:attribute name="font-style" use="required" type="xs:NCName"/>
            <xs:attribute name="font-weight" use="required" type="xs:NCName"/>
            <xs:attribute name="id" use="required" type="xs:NCName"/>
            <xs:attribute name="pizzicato" use="required" type="xs:NCName"/>
            <xs:attribute name="print-dot" use="required" type="xs:NCName"/>
            <xs:attribute name="print-leger" use="required" type="xs:NCName"/>
            <xs:attribute name="print-lyric" use="required" type="xs:NCName"/>
            <xs:attribute name="print-object" use="required" type="xs:NCName"/>
            <xs:attribute name="print-spacing" use="required" type="xs:NCName"/>
            <xs:attribute name="relative-x" use="required" type="xs:decimal"/>
            <xs:attribute name="relative-y" use="required" type="xs:decimal"/>
            <xs:attribute name="release" use="required" type="xs:decimal"/>
            <xs:attribute name="time-only" use="required" type="xs:NCName"/>
        </xs:complexType>
    </xs:element>
    <xs:element name="grace">
        <xs:complexType>
            <xs:attribute name="make-time" use="required" type="xs:decimal"/>
            <xs:attribute name="slash" use="required" type="xs:NCName"/>
            <xs:attribute name="steal-time-following" use="required" type="xs:integer"/>
            <xs:attribute name="steal-time-previous" use="required" type="xs:integer"/>
        </xs:complexType>
    </xs:element>
    <xs:element name="chord">
        <xs:complexType/>
    </xs:element>
    <xs:element name="cue">
        <xs:complexType/>
    </xs:element>
    <xs:element name="pitch">
        <xs:complexType>
            <xs:sequence>
                <xs:element ref="step"/>
                <xs:element ref="alter"/>
                <xs:element ref="octave"/>
            </xs:sequence>
        </xs:complexType>
    </xs:element>
    <xs:element name="step" type="xs:NCName"/>
    <xs:element name="alter" type="xs:decimal"/>
    <xs:element name="octave" type="xs:integer"/>
    <xs:element name="rest">
        <xs:complexType>
